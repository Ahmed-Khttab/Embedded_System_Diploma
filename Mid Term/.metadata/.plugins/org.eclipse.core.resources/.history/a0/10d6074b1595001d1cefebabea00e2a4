#include <stdio.h>

int main()
{
	int list[100];
	int list2[100];
	int num;

	printf("Enter the number of element = ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d",&num);

	for(int x=0;x<num;x++)
	{
		printf("Enter the element %d = ",x+1);
		fflush(stdin);
		fflush(stdout);
		scanf("%d",&list[x]);

	}
	for(int x=0;x<num;x++)
	{
		printf("%d ,",list[x]);

	}
}
